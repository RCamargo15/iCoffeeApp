<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9F90C8"
    tools:context=".activities.FinishPaymentScreen">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar4"
        android:layout_width="900dp"
        android:layout_height="166dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteY="1dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="310dp"
        android:layout_height="90dp"
        android:layout_marginStart="295dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="295dp"
        android:layout_marginBottom="40dp"
        android:text="iCoffee"
        android:textAlignment="center"
        android:textColor="#0E0505"
        android:textSize="48sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar4"
        app:layout_constraintEnd_toEndOf="@+id/toolbar4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/toolbar4"
        app:layout_constraintTop_toTopOf="@+id/toolbar4" />

    <TextView
        android:id="@+id/finishedPaymentMessage"
        android:layout_width="900dp"
        android:layout_height="305dp"
        android:layout_marginTop="357dp"
        android:layout_marginBottom="451dp"
        android:textAlignment="center"
        android:textColor="#090303"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6" />
</androidx.constraintlayout.widget.ConstraintLayout>